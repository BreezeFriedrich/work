          <pluginList>myDetectors.jar, yourDetectors.jar</pluginList>//optional BugDetector Jar files
        </configuration>
        -->
      </plugin>
      <!--项目源代码分析报告-->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-pmd-plugin</artifactId>
        <version>3.8</version>
        <configuration>
          <linkXRef>true</linkXRef>
          <targetJdk>${jdk-version}</targetJdk>
          <sourceEncoding>${project.build.sourceEncoding}</sourceEncoding>
          <minimumTokens>100</minimumTokens>
          <!--自定义规则
          <rulesets>
            <ruleset>rulesets/braces.xml</ruleset>
            <ruleset>rulesets/naming.xml</ruleset>
            <ruleset>rulesets/String.xml</ruleset>
          </rulesets>
          -->
        </configuration>
      </plugin>
      <!--生成在代码中发现的各种标记的报告-->
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>taglist-maven-plugin</artifactId>
        <version>2.4</version>
        <configuration>
          <encoding>UTF-8</encoding>
          <sourceFileLocale>en</sourceFileLocale>
          <emptyComments>false</emptyComments>
          <tagListOptions>
            <tagClasses>
              <tagClass>
                <displayName>Documentation Work</displayName>
                <tags>
                  <tag>
                    <matchString>DOCUMENT_ME</matchString>
                    <matchType>exact</matchType>
                  </tag>
                  <tag>
                    <matchString>NOT_YET_DOCUMENTED</matchString>
                    <matchType>exact</matchType>
                  </tag>
                </tags>
              </tagClass>
              <tagClass>
                <displayName>Code Work</displayName>
                <tags>
                  <tag>
                    <matchString>FIXME</matchString>
                    <matchType>exact</matchType>
                  </tag>
                  <tag>
                    <matchString>&lt;todo</matchString>
                    <matchType>exact</matchType>
                  </tag>
                  <tag>
                    <matchString>todo</matchString>
                    <matchType>ignoreCase</matchType>
                  </tag>
                </tags>
              </tagClass>
            </tagClasses>
          </tagListOptions>
          <!--向后兼容,之前版本tag配置
          <tags>
            <tag>fixme</tag>
            <tag>FixMe</tag>
            <tag>FIXME</tag>
            <tag>@todo</tag>
            <tag>todo</tag>
            <tag>TODO</tag>
            <tag>@deprecated</tag>
          </tags>
          -->
        </configuration>
      </plugin>
